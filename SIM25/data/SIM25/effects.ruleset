      "Road", "Road", "Local"
      "Building", "Super Highways", "City"
      "OutputType", "Trade", "Local"
      "CityTile", "Center", "Local"
    }
nreqs   =
    { "type", "name", "range"
      "Building", "Supermarket", "City"
      "Special", "Farmland", "Local"
    }

